{
  "api_version": "1.0",
  "base_url": "/api",
  "endpoints": [
    {
      "path": "/register/",
      "method": "POST",
      "description": "Регистрация нового пользователя",
      "request_body": {
        "username": "string",
        "password": "string"
      },
      "response": {
        "token": "string"
      }
    },
    {
      "path": "/history/",
      "method": "GET",
      "description": "Получение истории запросов пользователя",
      "authentication": "Required",
      "response": {
        "history": [
          {
            "request_type": "string",
            "timestamp": "datetime"
          }
        ]
      }
    },
    {
      "path": "/history/",
      "method": "DELETE",
      "description": "Удаление истории запросов пользователя",
      "authentication": "Required",
      "response": {
        "message": "string"
      }
    },
    {
      "path": "/change_password/",
      "method": "PATCH",
      "description": "Изменение ароля пользователя",
      "authentication": "Required",
      "request_body": {
        "new_password": "string"
      },
      "response": {
        "message": "string",
        "new_token": "string"
      }
    },
    {
      "path": "/text/",
      "method": "POST",
      "description": "Добавление нового текста",
      "authentication": "Required",
      "request_body": {
        "title": "string",
        "content": "string"
      },
      "response": {
        "id": "integer",
        "title": "string",
        "content": "string"
      }
    },
    {
      "path": "/text/{id}/",
      "method": "PATCH",
      "description": "Изменение текста",
      "authentication": "Required",
      "request_body": {
        "title": "string",
        "content": "string"
      },
      "response": {
        "id": "integer",
        "title": "string",
        "content": "string"
      }
    },
    {
      "path": "/text/{id}/",
      "method": "DELETE",
      "description": "Удаление текста",
      "authentication": "Required",
      "response": {
        "message": "string"
      }
    },
    {
      "path": "/text/{id}/",
      "method": "GET",
      "description": "Просмотр одного текста",
      "authentication": "Required",
      "response": {
        "id": "integer",
        "content": "string"
      }
    },
    {
      "path": "/text/",
      "method": "GET",
      "description": "Просмотр всех текстов",
      "authentication": "Required",
      "response": {
        "texts": [
          {
            "id": "integer",
            "content": "string"
          }
        ]
      }
    },
    {
      "path": "/encrypt/",
      "method": "POST",
      "description": "Шифрование текста",
      "authentication": "Required",
      "request_body": {
        "text": "string",
        "key": "string"
      },
      "response": {
        "encrypted": "string"
      }
    },
    {
      "path": "/decrypt/",
      "method": "POST",
      "description": "Дешифрование текста",
      "authentication": "Required",
      "request_body": {
        "text": "string",
        "key": "string"
      },
      "response": {
        "decrypted": "string"
      }
    }
  ]
}
